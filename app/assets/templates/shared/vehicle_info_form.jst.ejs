<h4 class="subtitle"> Vehicle Info</h4>

<ul class="vehicle-form-fields group">
    <% if (isSearch) { %>
      <li class="form-field">
        <input type="number" name="vehicle[year_start]" value="" placeholder="From?">
        <input type="number" name="vehicle[year_end]" value="" placeholder="To?">


    <% } else { %>
      <li class="form-field">
        <input type="number" name="vehicle[year]" value="" placeholder="Model Year">
      </li>
    <% } %>

  <li class="make-select form-field">

  </li>

  <li class="model-select inactive form-field">

  </li>
</ul>
