
<h1 class="title">Search</h1>



<form class="search-form">

  <ul class="search-options group">

    <li class="search-option" id="part_sales">Parts</li>
    <li class="search-option selected" id="vehicle_sales">Vehicles</li>
  </ul>
  <section class="vehicle-info" id="make-and-model"></section>

  <br>
  <section class="inactive" id="part-search">
    <h4 class="subtitle">Part Info</h4>
    <ul class="parts-info group">
      <li class="form-field part-categories">

      </li>
      <li class="form-field">
        <input type="text" name="part_sale[part_number]" placeholder="Part #" value="">
      </li>
      <li class="form-field">

        <input type="text" name="part_sale[part_type]" placeholder="What part is it?" value="">
      </li>

    </ul>
  </section>


  <button class="submit">Start looking</button>
</form>
