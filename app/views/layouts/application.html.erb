<!DOCTYPE html>
<html>
<head>
  <title>HemmingsClone</title>
  <%= stylesheet_link_tag    'application', media: 'all'%>
  <%= javascript_include_tag 'application'%>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash[:errors] %>
    <ul>
      <li><%= flash[:errors] %> </li>
    </ul>
  <% end %>

<% if logged_in? %>
  <h1> <%= current_user.email %> </h1>
  <br>
  <%= render partial: "shared/links_to" %>
<% else %>
  <%= my_button_to(new_session_url, "Log in", "get") %>
<% end %>
<%= yield %>

</body>
</html>
